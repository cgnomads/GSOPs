{
    "load_package_once": true,
    "enable": "houdini_version == '20.5'",
    "env": 
    [
        { 
            "GSOPS": "D:/Projects/gsops_renderer" 
        },
        {
            "GSOPS_VERSION": "2.7.0"
        },
        {
            "GSPLAT_RENDERER": 
            [
                { "houdini_os == 'windows'" : "$GSOPS/plugin/bin/windows/20.5" },
                { "houdini_os == 'linux'" : "$GSOPS/plugin/bin/linux/20.5" },
                { "houdini_os == 'macos'" : "$GSOPS/plugin/bin/macos/20.5" }
            ]
        }
    ],
    "hpath": 
    [ 
        "$GSOPS",
        "$GSPLAT_RENDERER"
    ]
}
